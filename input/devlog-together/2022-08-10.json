                            },
                            {
                                "type": "text",
                                "text": "new",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": ".\n\n"
                            },
                            {
                                "type": "link",
                                "url": "https:\/\/github.com\/jamii\/preimp\/blob\/078d30f9a290ae13b4a273eb3644218fd9e0e17a\/test\/end_to_end.test#L494-L543"
                            },
                            {
                                "type": "text",
