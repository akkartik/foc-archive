    {
        "user": "UC2A2ARPT",
        "type": "message",
        "ts": "1707279410.074789",
        "client_msg_id": "AF74D914-CAB3-4957-BED9-251A8A9A36B4",
        "text": "TypeScript is for the folks who want to build on those JS runtimes (and accrue all the benefits thereof) but also have the development aid of static typing. What more need it be?",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
