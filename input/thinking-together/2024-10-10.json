            "image_72": "https:\/\/secure.gravatar.com\/avatar\/dd63ba2bec5f7ecc9f23801caf0ace88.jpg?s=72&d=https%3A%2F%2Fa.slack-edge.com%2Fdf10d%2Fimg%2Favatars%2Fava_0012-72.png",
