    {
        "user": "U0378MDUG1Y",
        "type": "message",
        "ts": "1669878848.501169",
        "client_msg_id": "AAA2CABE-E0BE-4EEF-8784-642C650582C7",
        "text": "Thanks for sharing <@UJBAJNFLK>, I’m trying to tease out the differences between the two! \n\nSome follow up questions: Is end user programming inherently domain specific? What is the boundary between a domain expert vs. an end user?",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
            "avatar_hash": "1092a4f97098",
