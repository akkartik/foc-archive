    {
        "user": "U5STGTB3J",
        "type": "message",
        "ts": "1545297734.045400",
        "client_msg_id": "FCD4D8B2-2351-4725-BD16-7263F137F417",
        "text": "I collect PDFs on iCloud Drive, so I can read and highlight them with Preview on Mac, iPad, iPhone. I read books mostly in (i)Books on all devices.\nI then capture notes in either the Notes app or in iA Writer, also on all devices.\nMy reading list (links) I manage (together with my todos and projects) in Things on iPhone and iPad. Read articles and papers turn into references in my notes.\nSometimes I throw in a Pencil drawing on iPad  done in Linea Sketch or (currently testing) Nebo.\n\nThe transition from annotating external content and transforming it into my own notes is a very active part of my research right now, and I hope to switch to a different setup using our prototype early next year.",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
            "avatar_hash": "649181907e06",
            "image_72": "https:\/\/avatars.slack-edge.com\/2017-08-20\/228447816352_649181907e06ec450c64_72.jpg",
            "first_name": "Stefan",
            "real_name": "Stefan Lesser",
            "display_name": "Stefan",
            "team": "T5TCAFTA9",
            "name": "stefanlesser",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
        "thread_ts": "1545290663.026000",
        "parent_user_id": "UDJ2TKB99",
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "COZwK",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "I collect PDFs on iCloud Drive, so I can read and highlight them with Preview on Mac, iPad, iPhone. I read books mostly in (i)Books on all devices.\nI then capture notes in either the Notes app or in iA Writer, also on all devices.\nMy reading list (links) I manage (together with my todos and projects) in Things on iPhone and iPad. Read articles and papers turn into references in my notes.\nSometimes I throw in a Pencil drawing on iPad  done in Linea Sketch or (currently testing) Nebo.\n\nThe transition from annotating external content and transforming it into my own notes is a very active part of my research right now, and I hope to switch to a different setup using our prototype early next year."
                            }
                        ]
                    }
                ]
            }
        ]
    },
