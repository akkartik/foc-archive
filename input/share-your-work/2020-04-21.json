    {
        "user": "UDQBTJ211",
        "type": "message",
        "ts": "1587490088.156700",
        "client_msg_id": "26b9e28c-68b3-4bff-84c5-a7fdcd5fb9ee",
        "text": "One of the most frustrating things for me when I was a programmer was stepping through something then realising I had missed the key moment (where my expectations had diverged from the actual execution). Finding this moment is the key to begin understanding what's gone wrong, but it can be hard and slow to do.",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
            "avatar_hash": "2624b1e78c0a",
            "image_72": "https:\/\/avatars.slack-edge.com\/2019-02-14\/551655871797_2624b1e78c0a9eaed529_72.jpg",
            "first_name": "Chris",
            "real_name": "Chris Knott",
            "display_name": "",
            "team": "T5TCAFTA9",
            "name": "chrisknott",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
