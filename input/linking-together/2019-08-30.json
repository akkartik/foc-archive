    {
        "user": "UEQ6M68H0",
        "type": "message",
        "ts": "1567188905.029900",
        "client_msg_id": "258748a3-249c-43a9-a4ab-bfdf6a901127",
        "text": "Excel is a very successful product, but it isn't perfect, and there were many innovations on the way that have not received proper credit. Excel is now a fairly bloated product and i think successive versions are getting harder to use. The spreadsheet does have many fundamental features worth copying, and i have done so in my Beads language: 1) protected, extended arithmetic 2) automatic natural order of recalculation\/refresh, 3) no object creation, all things already exist already, they just might not have a value yet",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
            "avatar_hash": "326328f75c3f",
            "image_72": "https:\/\/avatars.slack-edge.com\/2019-02-05\/542651515888_326328f75c3f2a08544c_72.jpg",
            "first_name": "Edward",
            "real_name": "Edward de Jong",
            "display_name": "Edward de Jong \/ Beads Project",
            "team": "T5TCAFTA9",
            "name": "magicmouse94937",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "86W4",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "Excel is a very successful product, but it isn't perfect, and there were many innovations on the way that have not received proper credit. Excel is now a fairly bloated product and i think successive versions are getting harder to use. The spreadsheet does have many fundamental features worth copying, and i have done so in my Beads language: 1) protected, extended arithmetic 2) automatic natural order of recalculation\/refresh, 3) no object creation, all things already exist already, they just might not have a value yet"
                            }
