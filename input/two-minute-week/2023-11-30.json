        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
            "avatar_hash": "e5b277c64761",
            "image_72": "https:\/\/avatars.slack-edge.com\/2020-11-05\/1486125600916_e5b277c647611f765bfd_72.png",
            "first_name": "",
            "real_name": "Stew O'Connor",
            "display_name": "Stew O'Connor",
            "team": "T5TCAFTA9",
            "name": "mikeyo",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
        "thread_ts": "1701346694.830159",
        "parent_user_id": "U03U0SCU5LH",
        "blocks": [
