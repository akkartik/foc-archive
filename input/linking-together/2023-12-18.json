                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "Effects of monopoly to be precise "
                            },
                            {
                                "type": "link",
                                "url": "https:\/\/pluralistic.net\/2023\/11\/22\/who-wins-the-argument\/"
                            },
