    {
        "user": "U065XU6UY8H",
        "type": "message",
        "ts": "1699922319.480249",
        "client_msg_id": "2fe0d666-e794-4046-96ff-2d892ae63182",
        "text": "That's happening via the Worldwide Web Consortium (w3C) <https:\/\/www.w3.org\/immersive-web\/> - the body that negotiates agreements about how things like HTML, CSS, webGPU and webXR work.\n\nI'm personally responsible for working on the proposal for the 'HTML &lt;model&gt; element,' what I hope to be the basis for more spatial web content to be added in the coming years.",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
            "avatar_hash": "g1b893fcdf7c",
            "image_72": "https:\/\/secure.gravatar.com\/avatar\/1b893fcdf7c6624f35c06f4503e3f6f8.jpg?s=72&d=https%3A%2F%2Fa.slack-edge.com%2Fdf10d%2Fimg%2Favatars%2Fava_0004-72.png",
            "first_name": "Brandel",
            "real_name": "Brandel Zachernuk",
            "display_name": "Brandel Zachernuk",
            "team": "T5TCAFTA9",
            "name": "brandel",
            "is_restricted": false,
