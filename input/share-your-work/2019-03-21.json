        },
        "thread_ts": "1553216451.112800",
        "parent_user_id": "UEQ6M68H0",
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "JRGl",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "I prefer "
                            },
                            {
                                "type": "text",
                                "text": "#",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": " lately just because more languages use it. Seems useful to share linguistic conventions up and down the stack. Hadn't really considered the difficulty of typing; probably still shouldn't consider it. Emphasize reading over writing, and all that."
                            }
                        ]
                    }
                ]
            }
        ]
    },
