    {
        "user": "UCUSW7WVD",
        "type": "message",
        "ts": "1553223604.113000",
        "client_msg_id": "47933632-5777-4cbb-b86b-4dfa72303e4b",
        "text": "I prefer `#` lately just because more languages use it. Seems useful to share linguistic conventions up and down the stack. Hadn't really considered the difficulty of typing; probably still shouldn't consider it. Emphasize reading over writing, and all that.",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
            "avatar_hash": "6e649a383cf8",
            "image_72": "https:\/\/avatars.slack-edge.com\/2019-07-14\/687915485201_6e649a383cf8f9e366e3_72.png",
            "first_name": "Kartik",
            "real_name": "Kartik Agaram",
            "display_name": "",
            "team": "T5TCAFTA9",
            "name": "ak",
            "is_restricted": false,
