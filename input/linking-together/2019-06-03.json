    {
        "user": "UFV8P4472",
        "type": "message",
        "ts": "1559605437.018000",
        "client_msg_id": "ce82007e-4b79-4af9-94ad-b5eb8994a49b",
        "text": "Most modern web framework code goes through enough compilation and rendering steps that simply finding the widget instantiation code requires multiple hops, assisted by generated source maps.",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
        "source_team": "T5TCAFTA9",
        "user_profile": {
