        "user_profile": {
            "avatar_hash": "402a5ff55c77",
            "image_72": "https:\/\/avatars.slack-edge.com\/2023-07-05\/5533188035202_402a5ff55c77365d5465_72.jpg",
            "first_name": "Alexander",
            "real_name": "Alexander Bandukwala",
